{
    "expo": {
        "name": "WATSON fridge",
        "slug": "Watson-Fridge-APK",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./app/assets/icon.png",
        "splash": {
            "image": "./app/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.apk.watsonfridge",
            "supportsTablet": true
        },
        "android": {
            "package": "com.apk.watsonfridge"
        }
    }
}
